{
  "name": "beidou-plugin-react",
  "version": "0.1.0",
  "description": "beidou view react",
  "eggPlugin": {
    "name": "react"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run test-local",
    "test-local": "TEST_REPORTER=spec egg-bin test",
    "cov": "TEST_REPORTER=spec egg-bin cov",
    "lint": "eslint lib config app.js",
