    "history": "egg-bin history CHANGELOG.md",
    "precommit": "npm run lint"
  },
  "license": "ISC",
  "dependencies": {
    "js-beautify": "^1.6.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "autod": "2",
    "babel": "^6.5.2",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-beidou-client": "^0.0.5",
    "babel-preset-beidou-server": "^0.0.5",
    "egg": "^1.4.0",
    "egg-bin": "^3.4.2",
    "egg-mock": "^3.7.1",
    "eslint": "^3.6.1",
    "eslint-config-beidou": "^0.0.1",
    "husky": "^0.13.4",
    "react-redux": "^4.0.0",
    "redux": "~3.3.1",
    "should": "8",
    "supertest": "1",
    "webpack": "^1.13.2"
  },
  "files": [
    "lib",
    "config",
    "app.js"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "alicov": {
      "threshold": 90
    }
